generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Answer {
  YES
  NO
}

model CardResponse {
  id         String   @id @default(cuid())
  answer     Answer
  createdAt  DateTime @default(now())
  userAgent  String?
  ipAddress  String?
}
